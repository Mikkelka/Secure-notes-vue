{
  "compilerOptions": {
    "allowJs": true,
    "checkJs": false,
    "isolatedModules": true,
    "lib": ["ES2022", "DOM", "DOM.Iterable"],
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "noEmit": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "target": "ES2022",
    "types": ["vite/client"]
  },
  "include": ["src/**/*.js", "src/**/*.vue", "vite.config.js", "eslint.config.js"],
  "exclude": ["dist", "node_modules", "public/tinymce"]
}
