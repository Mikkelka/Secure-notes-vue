# Firebase Configuration
# Find these values in Firebase Console → Project Settings → General → Your apps
VITE_FIREBASE_API_KEY=your_api_key_here
VITE_FIREBASE_AUTH_DOMAIN=your-project.firebaseapp.com
VITE_FIREBASE_PROJECT_ID=your-project-id
VITE_FIREBASE_STORAGE_BUCKET=your-project.appspot.com
VITE_FIREBASE_MESSAGING_SENDER_ID=123456789
VITE_FIREBASE_APP_ID=1:123456789:web:abcdef123456

# Google Generative AI (Optional - for AI features)
# Get API key from: https://makersuite.google.com/app/apikey
VITE_GOOGLE_AI_API_KEY=your_google_ai_api_key_here

# Security Settings (Optional - defaults shown)
# Number of PBKDF2 iterations for key derivation (higher = more secure but slower)
VITE_PBKDF2_ITERATIONS=210000

# Encryption version prefix (don't change unless you know what you're doing)
VITE_ENCRYPTION_VERSION=securenotes_v1_

# Session Management (Optional - defaults shown)
# Session timeout in milliseconds (default: 30 minutes)
VITE_SESSION_TIMEOUT=1800000

# Warning time before session expires in milliseconds (default: 2 minutes)
VITE_WARNING_TIME=120000